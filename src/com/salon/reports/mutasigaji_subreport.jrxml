	<queryString><![CDATA[SELECT MUTASIID, WAKTU, IDKARYAWAN, KODETRANSAKSI, DEBET, KREDIT, 

(SELECT CASE WHEN D IS NULL THEN 0 ELSE D END FROM (SELECT SUM(KREDIT)-SUM(DEBET) AS D 
FROM (SELECT * FROM MUTASIKEUANGAN 
WHERE IDKARYAWAN = $P{idkaryawan}) AS MUTASI WHERE MUTASIID<=MK.MUTASIID) AS M) 
AS SALDO,

KETERANGAN FROM 
(SELECT * FROM MUTASIKEUANGAN 
WHERE IDKARYAWAN=$P{idkaryawan} AND WAKTU::DATE BETWEEN $P{drtanggal}::DATE AND $P{smptanggal}::DATE
ORDER BY MUTASIID) AS MK ORDER BY MUTASIID]]></queryString>

